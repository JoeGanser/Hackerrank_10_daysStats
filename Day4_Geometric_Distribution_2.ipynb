   "source": [
    "N = list(map(int,input().split()))\n",
    "M = int(input())\n",
    "p=N[0]/N[1]\n",
    "q=1-p\n",
    "\n",
    "probs = []\n",
    "for i in range(1,M+1):\n",
    "    probs.append(p*(q**(i-1)))\n",
    "\n",
    "probability = round(sum(probs),3)\n",
    "print(probability)\n",
    "\n"
   ]
  }
