   "source": [
    "l = float(input())\n",
    "k = int(input())\n",
    "\n",
    "def factorial(n):\n",
    "    if n == 0:\n",
    "        return 1\n",
    "    else:\n",
    "        return n * factorial(n-1)\n",
    "\n",
    "numerator = (2.718**(-l))*(l**k)\n",
    "denominator = factorial(k)\n",
    "prob = round(numerator/denominator,3)\n",
    "print(prob)"
   ]
  }
