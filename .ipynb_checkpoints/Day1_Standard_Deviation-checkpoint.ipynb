   "source": [
    "n = int(input().strip())\n",
    "X = [int(x) for x in input().strip().split()]\n",
    "\n",
    "mean = sum(X) / n\n",
    "variance = sum([((x - mean) ** 2) for x in X]) / n\n",
    "stddev = variance ** 0.5\n",
    "\n",
    "print(\"{0:0.1f}\".format(stddev))"
   ]
  }
