   "source": [
    "last_minute = int(input())\n",
    "number_waiting = int(input())\n",
    "mean = float(input())\n",
    "pop_std = float(input())\n",
    "\n",
    "\n",
    "X = last_minute/float(number_waiting)\n",
    "\n",
    "Z_score = (X-mean)/(pop_std/(number_waiting**(0.5)))\n",
    "\n",
    "import math\n",
    "cdf = (1 + math.erf(Z_score/(2**0.5)))/2\n",
    "prob = round(cdf,4)\n",
    "\n",
    "print(prob)"
   ]
  }
